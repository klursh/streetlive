<% content_for :title do %>
投稿動画
<% end %>

<div class="clearfix">
  <div class="text-center">
    <%= link_to "動画を投稿する", new_artist_movie_path(@artist),
                                  class: "btn btn-primary" %>
  </div>
</div>

<div id="my-movies-list">
  <%= render partial: "movies/list", locals: { movies: @artist.movies, show_artist: false, manage: true } %>
</div>

<% content_for :sidebar do %>
<%= render partial: "my/sidebar" %>
<% end %>
